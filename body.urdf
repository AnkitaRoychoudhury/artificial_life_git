<robot name = "robot">
    <link name="Torso">
        <inertial>
            <origin xyz="0 0 2.5" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 2.5" rpy="0 0 0"/>
            <geometry>
                <box size="1 3 1" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 2.5" rpy="0 0 0"/>
            <geometry>
                <box size="1 3 1" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_BackLeft" type="revolute">
       <parent link="Torso"/>
       <child  link="BackLeft"/>
       <origin rpy="0 0 0" xyz="0 -1.5 3" />
     <axis xyz="1 0 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="BackLeft">
        <inertial>
            <origin xyz="0 0 -2" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 -2" rpy="0 0 0"/>
            <geometry>
                <box size="0.5 0.5 2" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -2" rpy="0 0 0"/>
            <geometry>
                <box size="0.5 0.5 2" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_BackRight" type="revolute">
       <parent link="Torso"/>
       <child  link="BackRight"/>
       <origin rpy="0 0 0" xyz="0 0.5 0" />
     <axis xyz="1 0 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="BackRight">
        <inertial>
            <origin xyz="0 0 -1" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 -1" rpy="0 0 0"/>
            <geometry>
                <box size="0.5 0.5 2" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -1" rpy="0 0 0"/>
            <geometry>
                <box size="0.5 0.5 2" />
            </geometry>
        </collision>
    </link>
</robot>